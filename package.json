{
  "name": "mypleasure-kipp",
  "version": "0.2.0",
  "description": "Chrome browser extension to curate videos for MyPleasure.",
  "main": "main.jsx",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "build:manifest": "node ./config/manifest",
    "build:content": "NODE_ENV=production webpack -p --config ./src/content/webpack.config.js",
    "build:event": "NODE_ENV=production webpack -p --config ./src/event/webpack.config.js",
    "build:popup": "NODE_ENV=production webpack -p --config ./src/popup/webpack.config.js",
    "build:pages": "node ./config/pages",
    "build": "npm run build:manifest && npm run build:popup && npm run build:event && npm run build:content"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:dheavy/kipp.git"
  },
  "author": "Davy Peter Braun <hello@davybraun.com>",
  "license": "SEE LICENCE IN LICENSE",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "es6-promise": "^3.1.2",
    "immutable": "^3.7.6",
    "lodash": "^4.6.1",
    "normalize.scss": "^0.1.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-immutable-proptypes": "^1.7.0",
    "react-motion": "^0.4.2",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-saga": "^0.9.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-minifier": "^1.3.1",
    "jsdom": "^8.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
